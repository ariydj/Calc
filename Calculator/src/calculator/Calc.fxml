<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="160.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="calculator.FXMLDocumentController">
   <children>
      <TextField fx:id="label" alignment="CENTER_RIGHT" layoutY="2.0" prefHeight="50.0" prefWidth="500.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <HBox layoutY="50.0" prefHeight="30.0" prefWidth="500.0">
         <children>
            <Button fx:id="seven" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="30.0" prefWidth="125.0" text="7" />
            <Button fx:id="eight" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="30.0" prefWidth="125.0" text="8" />
            <Button fx:id="nine" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="30.0" prefWidth="125.0" text="9" />
            <Button fx:id="divNums" mnemonicParsing="false" prefHeight="30.0" prefWidth="125.0" text="/" />
         </children>
      </HBox>
      <HBox layoutY="80.0" prefHeight="30.0" prefWidth="500.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="80.0">
         <children>
            <Button fx:id="four" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="30.0" prefWidth="125.0" text="4" />
            <Button fx:id="five" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="30.0" prefWidth="125.0" text="5" />
            <Button fx:id="six" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="30.0" prefWidth="125.0" text="6" />
            <Button fx:id="multiNums" mnemonicParsing="false" prefHeight="30.0" prefWidth="125.0" text="x" />
         </children>
      </HBox>
      <HBox layoutX="5.0" layoutY="110.0" prefHeight="30.0" prefWidth="500.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="110.0">
         <children>
            <Button fx:id="one" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="30.0" prefWidth="125.0" text="1" />
            <Button fx:id="two" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="30.0" prefWidth="125.0" text="2" />
            <Button fx:id="three" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="30.0" prefWidth="125.0" text="3" />
            <Button fx:id="subNums" mnemonicParsing="false" prefHeight="30.0" prefWidth="125.0" text="-" />
         </children>
      </HBox>
      <HBox layoutX="15.0" layoutY="120.0" prefHeight="30.0" prefWidth="500.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="140.0">
         <children>
            <Button contentDisplay="RIGHT" mnemonicParsing="false" prefHeight="30.0" prefWidth="125.0" />
            <Button fx:id="zero" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="30.0" prefWidth="125.0" text="0" />
            <Button fx:id="equal" mnemonicParsing="false" onAction="#processOprtn" prefHeight="30.0" prefWidth="125.0" text="=" />
            <Button fx:id="addnUMS" contentDisplay="RIGHT" mnemonicParsing="false" prefHeight="30.0" prefWidth="125.0" text="+" />
         </children>
      </HBox>
   </children>
